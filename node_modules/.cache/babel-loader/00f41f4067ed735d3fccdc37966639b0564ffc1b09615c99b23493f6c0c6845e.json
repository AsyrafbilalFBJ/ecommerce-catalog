            <hr>\r\n                </div>\r\n                <div class=\"col-7\">\r\n                    <h2>{{ item.price }}</h2>\r\n                    <button>Buy Now</button>\r\n                    <button>Next Product</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'ProductDisplay',\r\n    data(){\r\n        return {list:undefined}\r\n    },\r\n    mounted(){\r\n        axios.get('https://fakestoreapi.com/products/')\r\n        .then((resp)=>{\r\n            this.list = resp.data;\r\n            console.warn(resp.data);\r\n        })\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}