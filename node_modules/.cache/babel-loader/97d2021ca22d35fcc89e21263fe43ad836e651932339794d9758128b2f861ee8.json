{"ast":null,"code":"const category = document.getElementById(\"category\");\nconst title = document.getElementById(\"title\");\nif (category.innerHTML == \"men's clothing\") {\n  title.classList.add(\"pria\");\n} else if (category.innerHTML == \"women's clothing\") {\n  title.classList.add(\"wanita\");\n}\nimport axios from 'axios';\nexport default {\n  name: 'ProductDisplay',\n  data() {\n    return {\n      list: undefined,\n      image: \"\",\n      title: \"\",\n      category: \"\",\n      rate: \"\",\n      description: \"\",\n      price: \"\",\n      count: 0\n    };\n  },\n  methods: {\n    getData() {\n      axios.get('https://fakestoreapi.com/products/').then(resp => {\n        this.list = resp.data[this.count];\n        this.image = this.list['image'];\n        this.title = this.list['title'];\n        this.category = this.list['category'];\n        this.rate = this.list['rating']['rate'];\n        this.description = this.list['description'];\n        this.price = this.list['price'];\n        console.warn(this.count);\n      });\n    },\n    increment() {\n      if (this.count <= 18) {\n        this.count++;\n        this.getData();\n        console.warn(this.count);\n      } else {\n        this.count = 0;\n        this.getData();\n      }\n    }\n  },\n  mounted() {\n    axios.get('https://fakestoreapi.com/products/').then(resp => {\n      this.list = resp.data[this.count];\n      this.image = this.list['image'];\n      this.title = this.list['title'];\n      this.category = this.list['category'];\n      this.rate = this.list['rating']['rate'];\n      this.description = this.list['description'];\n      this.price = this.list['price'];\n      console.warn(this.color.pria);\n    });\n  },\n  computed: {\n    adjustStyle() {\n      return {\n        pria: true\n      };\n    }\n  }\n};","map":{"version":3,"mappings":"AA2CA,MAAMA,QAAO,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC;AACpD,MAAMC,KAAI,GAAIF,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC;AAC9C,IAAGF,QAAQ,CAACI,SAAQ,IAAK