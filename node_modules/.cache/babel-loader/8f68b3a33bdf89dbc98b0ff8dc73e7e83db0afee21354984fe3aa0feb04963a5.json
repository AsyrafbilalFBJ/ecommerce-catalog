{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'ProductDisplay',\n  data() {\n    return {\n      list: undefined,\n      image: \"\",\n      title: \"\",\n      category: \"\",\n      rate: \"\",\n      description: \"\",\n      price: \"\",\n      count: 0\n    };\n  },\n  methods: {\n    getData() {\n      axios.get('https://fakestoreapi.com/products/').then(resp => {\n        this.list = resp.data[this.count];\n        this.image = this.list['image'];\n        this.title = this.list['title'];\n        this.category = this.list['category'];\n        this.rate = this.list['rating']['rate'];\n        this.description = this.list['description'];\n        this.price = this.list['price'];\n        console.warn(this.count);\n      });\n    },\n    increment() {\n      if (this.count <= 18) {\n        this.count++;\n        this.getData();\n        console.warn(this.count);\n      } else {\n        this.count = 0;\n        this.getData();\n      }\n    }\n  },\n  mounted() {\n    axios.get('https://fakestoreapi.com/products/').then(resp => {\n      this.list = resp.data[this.count];\n      this.image = this.list['image'];\n      this.title = this.list['title'];\n      this.category = this.list['category'];\n      this.rate = this.list['rating']['rate'];\n      this.description = this.list['description'];\n      this.price = this.list['price'];\n      // console.warn(this.color.pria);\n    });\n  }\n  // computed: {\n  //     backgroundEmpty(){\n  //         return `background-image: url(\"./assets/bg_empty.png\");`;\n  //     }\n  // }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAEC,SAAS;MACfC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACX;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,OAAO,GAAE;MACLb,KAAK,CAACc,GAAG,CAAC,oCAAoC,EACzCC,IAAI,CAAEC,IAAI,IAAK;QACZ,IAAI,CAACb,IA