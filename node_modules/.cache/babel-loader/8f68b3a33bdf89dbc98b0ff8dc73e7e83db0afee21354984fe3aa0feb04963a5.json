ide_button u-sv3\"\r\n                        v-bind:class=\"{ show_button: category !== 'men\\'s clothing' && category !== 'women\\'s clothing' }\">This product\r\n                        is unavailable to show</span>\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-12\">\r\n                    <button v-on:click=\"increment\" class=\"hide_button\"\r\n                        v-bind:class=\"{ show_button: category !== 'men\\'s clothing' && category !== 'women\\'s clothing' }\">Next\r\n                        Product</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        \r\n        <div class=\"col-small-3 col-medium-3 col-5\" v-bind:class=\"{ hide: category !== 'men\\'s clothing' && category !== 'women\\'s clothing' }\">\r\n            <img class=\"p-image u-align--center\" v-bind:src=\"image\" alt=\"\">\r\n        </div>\r\n        <div class=\"col-small-1 col-medium-3 col-6\"  v-bind:class=\"{ hide: category !== 'men\\'s clothing' && category !== 'women\\'s clothing' }\">\r\n            <div class=\"row p-divider u-align-text--left\">\r\n                <div class=\"col-6\">\r\n                    <h2 id=\"title\" v-bind:class=\"{ pria_text: category === 'men\\'s clothing', wanita_text: category === 'women\\'s clothing'}\">{{ title }}</h2>\r\n                    <div class=\"row\">\r\n                        <div class=\"col-3\">\r\n                            <p id=\"category\">{{ category }}</p>\r\n                        </div>\r\n                        <div class=\"col-3 u-align-text--right\">\r\n                            <p>{{ rate }}/5</p>\r\n                        </div>\r\n                    </div>\r\n                    <hr>\r\n                </div>\r\n                <div class=\"col-6 u-sv3\">\r\n                    <p>{{ description }}</p>\r\n                    <hr>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                    <h2 id=\"price\" v-bind:class=\"{ pria_text: category === 'men\\'s clothing', wanita_text: category === 'women\\'s clothing'}\">${{ price }}</h2>\r\n                    <div class=\"row\">\r\n                        <div class=\"col-3 u-align--center button\">\r\n                            <button id=\"buy\" v-bind:class=\"{ pria_button: category === 'men\\'s clothing', wanita_button: category === 'women\\'s clothing'}\">Buy Now</button>\r\n                        </div>\r\n                        <div class=\"col-3 u-align--center button\">\r\n                            <button v-on:click=\"increment\" id=\"next\" v-bind:class=\"{ pria_border: category === 'men\\'s clothing', wanita_border: category === 'women\\'s clothing'}\">Next Product</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"col-small-1 col-medium-3 col-1\" v-bind:class=\"{ hide: category !== 'men\\'s clothing' && category !== 'women\\'s clothing' }\">\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'ProductDisplay',\r\n    data() {\r\n        return {\r\n            list: undefined,\r\n            image: \"\",\r\n            title: \"\",\r\n            category: \"\",\r\n            rate: \"\",\r\n            description: \"\",\r\n            price: \"\",\r\n            count: 0,\r\n        }\r\n    }, \r\n    methods: {\r\n        getData(){\r\n            axios.get('https://fakestoreapi.com/products/')\r\n                .then((resp) => {\r\n                    this.list = resp.data[this.count];\r\n                    this.image = this.list['image'];\r\n                    this.title = this.list['title'];\r\n                    this.category = this.list['category'];\r\n                    this.rate = this.list['rating']['rate'];\r\n                    this.description = this.list['description'];\r\n                    this.price = this.list['price'];\r\n                    console.warn(this.count);\r\n                })\r\n        },\r\n        increment() {\r\n            if (this.count <= 18) {\r\n                this.count++\r\n                this.getData();\r\n                console.warn(this.count);\r\n            } else{\r\n                this.count = 0;\r\n                this.getData();\r\n            }\r\n        },\r\n    },\r\n    mounted(){\r\n        axios.get('https://fakestoreapi.com/products/')\r\n            .then((resp) => {\r\n                this.list = resp.data[this.count];\r\n                this.image = this.list['image'];\r\n                this.title = this.list['title'];\r\n                this.category = this.list['category'];\r\n                this.rate = this.list['rating']['rate'];\r\n                this.description = this.list['description'];\r\n                this.price = this.list['price'];\r\n                // console.warn(this.color.pria);\r\n            })\r\n    },\r\n    // computed: {\r\n    //     backgroundEmpty(){\r\n    //         return `background-image: url(\"./assets/bg_empty.png\");`;\r\n    //     }\r\n    // }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}